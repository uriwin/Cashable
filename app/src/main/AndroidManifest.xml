<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapplication" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo2"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:label="@string/app_name"
        android:theme="@style/AppTheme.Fullscreen">
        <activity android:name=".sampledata.mygame.Activities.FrontPages.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".sampledata.mygame.Activities.FrontPages.Modes"></activity>
        <activity android:name=".sampledata.mygame.Activities.FrontPages.Shop"></activity>
        <activity android:name=".sampledata.mygame.Activities.FrontPages.Levels"></activity>
        <activity android:name=".sampledata.mygame.Activities.GameModesActivities.Infinity"></activity>
        <activity android:name=".sampledata.mygame.Activities.GameModesActivities.OneVsOne"></activity>
        <activity android:name=".sampledata.mygame.Activities.GameModesActivities.AgainstTime"></activity>
        <activity android:name=".sampledata.mygame.Activities.FrontPages.Instructions"></activity>
    </application>

</manifest>